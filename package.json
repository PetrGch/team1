{
  "name": "team1",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "6.10.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server-build": "webpack --progress --config webpack.serverconf.js",
    "dev:client-build": "webpack --progress --config webpack.devconf.js",
    "dev:run": "nodemon ./bin/server.js",

    "lint": "npm-run-all lint:*",
    "lint:style": "stylelint views/**/*.pcss",
    "lint:js": "eslint --ignore-path .gitignore .",
    "test": "npm-run-all lint test:run",
    "test:run": "mocha-webpack --webpack-config webpack.testconf.js \"test/**/*.test.js\"",

    "selenium:install": "./node_modules/.bin/selenium-standalone install",
    "selenium:start": "./node_modules/.bin/selenium-standalone start",
    "selenium:test": "./node_modules/.bin/wdio wdio.conf.js",

    "prestart": "npm i",
    "start": "NODE_ENV=production npm-run-all prod:*",
    "prod:build": "webpack --config webpack.prodconf.js && webpack --config webpack.serverconf.js",
    "prod:deploy": "surge -p ./public -d team1.surge.sh",
    "prod:run": "node ./bin/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urfu-2016/team1.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/urfu-2016/team1/issues"
  },
  "homepage": "https://github.com/urfu-2016/team1#readme",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "^6.22.0",
    "body-parser": "~1.16.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "cookie-parser": "~1.4.3",
    "core-decorators": "0.17.0",
    "css-loader": "^0.26.1",
    "debug": "~2.6.0",
    "enzyme": "2.8.0",
    "eslint": "3.7.0",
    "eslint-config-hrundel": "latest",
    "eslint-config-standard-jsx": "3.3.0",
    "eslint-loader": "1.6.3",
    "eslint-plugin-react": "6.10.0",
    "express": "~4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "jsonwebtoken": "7.3.0",
    "mocha": "3.1.0",
    "mocha-webpack": "0.7.0",
    "morgan": "~1.7.0",
    "normalize.css": "5.0.0",
    "npm-run-all": "4.0.2",
    "pg": "6.1.4",
    "postcss-custom-media": "5.0.1",
    "postcss-loader": "^1.3.3",
    "postcss-smart-import": "^0.6.9",
    "precss": "^1.4.0",
    "prop-types": "15.5.4",
    "react": "15.5.0",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.5.0",
    "react-redux": "5.0.3",
    "react-router": "^3.0.2",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-thunk": "2.2.0",
    "remote-static": "1.1.0",
    "selenium-webdriver": "3.3.0",
    "sequelize": "3.30.2",
    "serve-favicon": "~2.3.2",
